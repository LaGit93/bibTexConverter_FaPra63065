@InCollection{Khan:2009:IHA,
  author =       "Asifullah Khan and Imran Usman",
  title =        "Intelligent Perceptual Shaping in Digital
                 Watermarking",
  booktitle =    "Information Hiding and Applications",
  publisher =    "Springer",
  year =         "2009",
