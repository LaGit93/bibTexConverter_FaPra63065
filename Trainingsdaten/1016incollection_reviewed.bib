@InCollection{Card:2007:GPTP,
  author =       "Stuart W. Card and Chilukuri K. Mohan",
  title =        "Towards an Information Theoretic Framework for Genetic
                 programming",
  booktitle =    "Genetic Programming Theory and Practice {V}",
  year =         "2007",
